      "examples": [
        {
          "title": "Request-Example:",
          "content": "    {\n\"description\" : \"Place Stanislas\",\n\"latitude\" : \"23.5487° E\",\n\"longitude\" : \"5.2136° E\",\n\"link\" : \"https://google.com\"\n    }",
